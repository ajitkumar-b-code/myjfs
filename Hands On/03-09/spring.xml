<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">


<bean id="student" class="com.myspr.demo.Student" autowire="constructor">
<!-- <property name="name" value="JOHN"></property>  -->
<constructor-arg value="20"></constructor-arg>
<constructor-arg type="int" value="1000"></constructor-arg>
</bean>

<bean id="address" class="com.myspr.demo.Address" >
<property name="state" value="karnataka"></property>
<property name="city" value="bijapur"></property>
</bean>

<context:annotation-config/>
</beans>
